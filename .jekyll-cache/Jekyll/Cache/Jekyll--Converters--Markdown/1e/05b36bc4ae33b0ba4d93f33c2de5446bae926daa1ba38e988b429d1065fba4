<span class="n">FROM</span> <span class="n">ANIMAL_INS</span>
<span class="n">ORDER</span> <span class="n">BY</span> <span class="n">NAME</span><span class="p">,</span> <span class="n">DATETIME</span> <span class="n">DESC</span><span class="p">;</span>
